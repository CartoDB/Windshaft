        "cartodb-psql": "^0.10.1",
        "d3-queue": "^3.0.7",
        "debug": "^3.1.0",
        "dot": "~1.1.2",
        "grainstore": "1.8.2",
        "redis-mpool": "^0.5.0",
        "request": "^2.83.0",
        "semver": "~5.5.0",
        "step": "1.0.0",
        "tilelive-mapnik": "cartodb/tilelive-mapnik#0.6.18-cdb6",
        "torque.js": "~2.16.2",
        "underscore": "~1.6.0"
    },
    "devDependencies": {
        "express": "4.16.2",
        "istanbul": "0.4.5",
        "jshint": "~2.9.5",
        "mocha": "5.0.1",
        "redis": "~2.8.0"
    },
    "scripts": {
        "preinstall": "make pre-install",
        "test": "make test-all",
        "docker-install": "sudo apt install docker.io && sudo usermod -aG docker $(whoami)",
        "docker-pull": "docker pull cartoimages/windshaft-testing",
        "docker-test": "docker run -v `pwd`:/srv cartoimages/windshaft-testing bash docker-test.sh && docker ps --filter status=dead --filter status=exited -aq | xargs -r docker rm -v",
        "docker-build": "docker build -t cartoimages/windshaft-testing .",
        "docker-publish": "docker push cartoimages/windshaft-testing"
    },
    "engines": {
        "node": ">=6.9",
        "npm": ">=3.10"
    }
