        "Raul Ochoa <rochoa@cartodb.com>",
        "Daniel Garcia Aubert <dgaubert@carto.com>"
    ],
    "dependencies": {
        "@carto/mapnik": "3.6.2-carto.3",
        "@carto/tilelive-bridge": "cartodb/tilelive-bridge#2.5.1-cdb2",
        "@mapbox/sphericalmercator": "^1.0.5",
        "@mapbox/tilelive": "^6.0.0",
        "abaculus": "cartodb/abaculus#2.0.3-cdb3",
        "canvas": "cartodb/node-canvas#1.6.2-cdb2",
